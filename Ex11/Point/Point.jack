class Point {
    field int x, y;
    static int pointCount;

    method int distance(Point other) {
        var int dx, dy;
        let dx = x - other.getx();
        let dy = y- other.gety();
        return Math.sqrt((dx*dx)+(dy*dy));
    }

    method int getx() {return x; }

    method int gety() {return y; }
}